  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>SuperStore_DW_DS2</DataSourceName>
        <CommandText>SELECT b.MonthYear, d.ProductCategoryName, c.fname + c.lname AS CustomerName, a.FreightValue, a.ReviewScore, a.Price, a.PaymentValue
FROM     FactTable AS a INNER JOIN
                  DimDate AS b ON a.order_estimated_delivery_DateKey = b.DateKey INNER JOIN
                  Dim_Product AS d ON a.ProductID = d.ProductID INNER JOIN
                  Dim_Customer AS c ON a.CustomerID = c.CustomerID
WHERE  (a.PaymentValue &gt; 0)</CommandText>
      </Query>
      <Fields>
        <Field Name="MonthYear">
          <DataField>MonthYear</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ProductCategoryName">

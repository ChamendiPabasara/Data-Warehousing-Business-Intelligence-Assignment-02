  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource3</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@customer_state">
            <Value>=Parameters!customer_state.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@customer_city">
            <Value>=Parameters!customer_city.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT b.MonthYear, c.fname + c.lname AS CustomerName, c.customer_state AS RegionName, c.customer_city AS City, a.ReviewScore, a.PaymentValue
FROM     FactTable AS a INNER JOIN
                  DimDate AS b ON a.order_delivered_customer_DateKey = b.DateKey INNER JOIN
                  Dim_Customer AS c ON a.CustomerID = c.CustomerID
WHERE  (a.PaymentValue &gt; 0) AND (c.customer_state IN (@customer_state)) AND (c.customer_city IN (@customer_city))</CommandText>
      </Query>
      <Fields>
        <Field Name="MonthYear">
          <DataField>MonthYear</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
